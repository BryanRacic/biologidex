; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="biologidex-client"
run/main_scene="res://scenes/login/login.tscn"
config/features=PackedStringArray("4.5", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Bootstrap="*res://bootstrap.gd"
NavigationManager="*res://features/navigation/navigation_manager.gd"
TokenManager="*res://features/server_interface/auth/token_manager.gd"
APIManager="*res://features/server_interface/api/api_manager.gd"
DexDatabase="*res://features/database/dex_database.gd"
TreeCache="*res://features/tree/tree_cache.gd"
SyncManager="*res://features/database/sync_manager.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/size/mode=2
window/size/initial_position_type=3
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/aspect="expand"
window/handheld/orientation=6

[editor_plugins]

enabled=PackedStringArray("res://addons/godot-file-access-web/addons/FileAccessWeb/plugin.cfg")

[gui]

theme/default_font_multichannel_signed_distance_field=true

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
